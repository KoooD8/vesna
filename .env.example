# Example environment variables for AI Agents Stack
# Copy to .env and adjust values

# Qdrant
AI_STACK_QDRANT_URL=http://localhost:6333
AI_STACK_QDRANT_COLLECTION=ai_research
AI_STACK_EMB_MODEL=sentence-transformers/all-MiniLM-L6-v2
AI_STACK_QDRANT_BATCH=128

# Obsidian config path (YAML file with vault_path/folders)
AI_STACK_CONFIG=/Users/onopriychukpavel/Library/Mobile Documents/iCloud~md~obsidian/Documents/Version1/ai_agents_stack.config.yaml
# Default vault folder if YAML is missing or doesn't specify vault_path
AI_STACK_DEFAULT_VAULT=/Users/onopriychukpavel/Library/Mobile Documents/iCloud~md~obsidian/Documents/Version1

# Caching and rate limiting
AI_STACK_HTTP_CACHE=1
AI_STACK_HTTP_CACHE_TTL=300
AI_STACK_RATE_INTERVAL=0.5

# Logging
AI_STACK_JSON_LOGS=0

# External APIs (optional)
SERPAPI_KEY=
REDDIT_CLIENT_ID=
REDDIT_CLIENT_SECRET=
TAVILY_API_KEY=

# LLM providers (optional)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Whisper transcription (optional)
WHISPER_MODEL=small
WHISPER_DEVICE=auto
WHISPER_COMPUTE=int8

